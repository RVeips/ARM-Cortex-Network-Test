{
    "version": 4,
    "env": {
        "ProjectName": "CortexNetworkTestApp",
        "ProjectRoot": "${workspaceFolder}/${ProjectName}",
        "ProjectVendorRoot": "${ProjectRoot}/vendor"
    },
    "configurations": [
        {
            "name": "GCC",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "compilerPath": "arm-none-eabi-g++.exe",
            "intelliSenseMode": "gcc-arm",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ]
}